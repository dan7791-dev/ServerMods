<?xml version="1.0" encoding="UTF-8"?>
<!-- salvageHarvest is what comes out of it while I wrench it apart -->
<configs>
	<remove xpath="/blocks/block[@name='solarbank']/drop[contains(@tag,'salvageHarvest')]" />
	<append xpath="/blocks/block[@name='solarbank']">
		<property name="MaxDamage" value="250"/>
		<drop event="Harvest" name="glassBulletproofBlockVariantHelper" count="1" prob=".8" tag="salvageHarvest"/>
		<drop event="Harvest" name="solarCell" count="1,4" prob=".4" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceMechanicalParts" count="1,2" prob=".4" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceElectricParts" count="1,2" prob=".4" tag="salvageHarvest"/>
	</append>

	<remove xpath="/blocks/block[@name='cntCollapsedSolarbank']/drop[contains(@tag,'salvageHarvest')]" />
	<append xpath="/blocks/block[@name='cntCollapsedSolarbank']">
		<drop event="Harvest" name="glassBulletproofBlockVariantHelper" count="1" prob=".8" tag="salvageHarvest"/>
		<drop event="Harvest" name="solarCell" count="1,4" prob=".4" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceMechanicalParts" count="1,2" prob=".4" tag="salvageHarvest"/>
		<drop event="Harvest" name="resourceElectricParts" count="1,2" prob=".4" tag="salvageHarvest"/>
	</append>
</configs>