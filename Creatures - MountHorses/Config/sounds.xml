<configs>
<append xpath="/Sounds">
<!-- Entities -->
<SoundDataNode name="HorseTrotSound"> <!--<AudioSource name="Sounds/AudioSource_Screamer"/>--> <AudioSource name="Sounds/AudioSource_Vehicle"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorses.unity3d?HorseWalkLoop" />
	<!--<Channel name="Mouth"/>-->
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="0.95"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value=".1"/> 
</SoundDataNode>
<SoundDataNode name="HorseGallopSound"> 
<AudioSource name="Sounds/AudioSource_Vehicle"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorses.unity3d?HorseGallopSoundPedal" />
<!--
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorses.unity3d?HorseWalkSoundPedal" />-->
	<!--<Channel name="Mouth"/>-->
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="0.75"/>
  <HighestPitch name="1.25"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value=".1"/> 
</SoundDataNode>
<SoundDataNode name="HorseSpeakSound"> <!--<AudioSource name="Sounds/AudioSource_Screamer"/>--> <AudioSource name="Sounds/AudioSource_Vehicle"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorses.unity3d?HorseSpeakSound"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorses.unity3d?HorseSpeakSound2"/>
	<!--<Channel name="Mouth"/>-->
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="0.95"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value=".1"/> 
</SoundDataNode>

<SoundDataNode name="HorseTamingCompleteSound"> <!--<AudioSource name="Sounds/AudioSource_Screamer"/>--> <AudioSource name="Sounds/AudioSource_Vehicle"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorses.unity3d?HorseTamingCompleteSound"/>
	<!--<Channel name="Mouth"/>-->
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="0.95"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value=".1"/> 
</SoundDataNode>

<!-- tests -->
