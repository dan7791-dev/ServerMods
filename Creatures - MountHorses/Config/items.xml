<item name="foodApple">
	<property name="Tags" value="food"/>
	<property name="HoldType" value="31"/>
	<property name="CustomIcon" value="AppleIcon" />
	<property name="DisplayType" value="food"/>
	<property name="Meshfile" value="#@modfolder:Resources/TelricsHorses.unity3d?AppleItemPrefab"/>
	<property name="DropMeshfile" value="#@modfolder:Resources/TelricsHorses.unity3d?AppleItemPrefab"/>
	<property name="Material" value="Morganic"/>
	<property name="Stacknumber" value="1000"/> <!-- STK food -->
	<property name="EconomicValue" value="24"/>
	<property class="Action0">
		<property name="Class" value="UseOther"/>
		<property name="Delay" value="2.1"/>
		<property name="Use_time" value="..."/>
		<property name="Sound_start" value="player_eating"/>
	</property>
	<property name="Group" value="Food/Cooking,CFFood/Cooking"/>

	<effect_group tiered="false">
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffBrownHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseBrown"/>
		</triggered_effect>
			
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffBlackHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseBlack"/>
		</triggered_effect>
		
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffYellowHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseYellow"/>
		</triggered_effect>
		
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffAlbinoHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseAlbino"/>
		</triggered_effect>
		
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffCreamHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseCream"/>
		</triggered_effect>
		
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffMidnightHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseMidnight"/>
		</triggered_effect>
		
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffGrayHorseTamingComplete" target="other">
			<requirement name="EntityTagCompare" tags="horseGray"/>
		</triggered_effect>
	</effect_group>
</item>

<!-- mounts -->
<item name="vehicleBrownHorsePlaceable">
	<property name="Extends" value="vehicleMinibikePlaceable"/>
	<property name="Tags" value="vehicle"/>
	<property name="DescriptionKey" value="vehicleBrownHorsePlaceableDesc"/>
	<property name="CustomIcon" value="VehicleHorseIcon" />
	<property name="Meshfile" value="#@modfolder:Resources/TelricsHorses.unity3d?BrownHorseVehiclePrefab"/>
	<property name="SellableToTrader" value="true"/>
	<property name="EconomicValue" value="3000"/>
	<property class="Action1">
		<property name="Vehicle" value="vehicleBrownHorse"/>
		<property name="VehicleSize" value="2.5, 2.1, 5"/>
	</property>
	<effect_group name="vehicle4x4TruckPlaceable" tiered="false">
		<passive_effect name="ModSlots" operation="base_set" value="4"/>
		<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
		<passive_effect name="DegradationMax" operation="base_set" value="7500"/>
	</effect_group>
</item>
